{
  "runtime": "python3.12",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/login", "destination": "/static/login.html" },
    { "source": "/", "destination": "/static/index.html" },
    { "source": "/(.*)", "destination": "/api/index.py" }
  ]
}
